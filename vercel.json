{
    "version": 2,
    "builds": [
      {
        "src": "/*",
        "use": "@vercel/static"
      },
      {
        "src": "/js/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/js/server.js", "dest": "js/server.js" }
    ]
  }