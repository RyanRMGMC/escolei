{
    "version": 2,
    "builds": [
      {
        "src": "/*",
        "use": "@vercel/static"
      },
      {
        "src": "js/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/js/(.*)", "dest": "js/server.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }