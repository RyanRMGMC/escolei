{
    "version": 2,
    "builds": [
      {
        "src": "Projeto ESCOLEI/*",
        "use": "@vercel/static"
      },
      {
        "src": "server/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/server/(.*)", "dest": "server/$1" },
      { "src": "/(.*)", "dest": "Projeto ESCOLEI/$1" }
    ]
  }